set notification-error-bg       "#d6ce7d" #08
set notification-error-fg       "#ece8be" #00

set notification-warning-bg     "#e4de9f" #08
set notification-warning-fg     "#ece8be" #00

set highlight-color             "#2a4f81" #0A
set highlight-active-color      "#1f1b3f" #0D

set completion-highlight-fg     "#1f1b3f"  #02
