set notification-bg             "#cac054" #0B
set notification-fg             "#ece8be" #00

set recolor-lightcolor          "#162b24" #00
set recolor-darkcolor           "#c6e1a6" #06
set recolor                     "true"

# setting recolor-keep true will keep any color your pdf has.
# if it is false, itll just be black and white
set recolor-keephue             "false"
